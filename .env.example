# Enable or disable code analysis ("true" or "false")
ENABLE_ANALYSIS=true

# GitHub and OpenAI credentials (DO NOT COMMIT .env FILE WITH REAL VALUES)
OPENAI_API_KEY=your_openai_api_key_here
GITHUB_TOKEN=your_github_token_here

# Repository to analyze (format: owner/repo)
REPO=your_github_username/your_repo
GITHUB_BRANCH=feature/your-branch-name